class Solution {
    public boolean checkIfExist(int[] arr) {
        int n=arr.length;
        boolean ans=false;

        for(int i=0 ; i<n ; i++){
            for(int j=0 ; j<n ; j++){
                if(i!=j){
                    if(2*arr[i]==arr[j]){
                        ans=true;
                    }
                }
            }
        }
        return ans;
    }
}
