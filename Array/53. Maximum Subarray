class Solution {
    public int maxSubArray(int[] nums) {
        int maxSum=Integer.MIN_VALUE;
        int n=nums.length;

        for(int st=0 ; st<n ; st++){
            int currSum=0;
            for(int end=st ; end<n ; end++){
                currSum+=nums[end];
                maxSum=Math.max(currSum,maxSum);
            }
        }
        return maxSum;
    }
}
