class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int[] ans = new int[m + n];
        int i = 0, j = 0, k = 0;

        while (i < m && j < n) {
            if (nums1[i] < nums2[j]) {
                ans[k++] = nums1[i++];
            } else {
                ans[k++] = nums2[j++];
            }
        }

        while (i < m) {
            ans[k++] = nums1[i++];
        }

        while (j < n) {
            ans[k++] = nums2[j++];
        }

        // Copy merged array back to nums1
        for (i = 0; i < m + n; i++) {
            nums1[i] = ans[i];
        }

        // Printing the merged array
        for (i = 0; i < m + n; i++) {
            System.out.print(ans[i] + ",");
        }
    }
}
